{#
/**
 * array 10 point choice Html
 *
 * @var $sColumns   : the columns, generated with the view columns/col.php
 * @var $sHeaders   : the headers, generated with the view rows/cell/thead.php
 * @var $sRows      : the rows, generated with the view rows/answer_row.php
 * @var $extraclass
 * @var $answerwidth
 */
#}
<!-- Array 10 point choice -->

<div class="ls-table-wrapper table-card" aria-labelledby="ls-question-text-{{ basename }}">
  <table class="{{ coreClass }} {{ extraclass }} table table-10-point-array matrix-compact pretty-table">
    <colgroup class="col-responses">
      <col class="col-answers" style="width: {{ answerwidth }}%;" />
      {{ sColumns }}
    </colgroup>
    <thead>
      <tr class="ls-heading">
        {{ sHeaders }}
      </tr>
    </thead>
    <tbody>
      {{ sRows }}
    </tbody>
  </table>
</div>
